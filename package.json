{
  "name": "@neos-project/react-ui-components",
  "version": "1.0.0",
  "description": "The UI components which power the Neos backend application.",
  "main": "./lib",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "check-dependencies && npm run clean",
    "test": "npm run lint",
    "build": "cross-env NODE_ENV=production npm run babel && npm run css",
    "babel": "babel ./src --out-dir ./lib",
    "css": "cpx './src/**/*.css' ./lib",
    "clean": "rimraf ./lib ./dist",
    "lint": "eslint src && stylelint 'src/**/*.css'"
  },
  "devDependencies": {
    "classnames": "^2.2.3",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "check-dependencies": "^0.12.0",
    "cpx": "^1.3.1",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.0.1",
    "lodash.omit": "^4.3.0",
    "monet": "^0.8.10",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.2.1",
    "react-click-outside": "^2.1.0",
    "react-collapse": "^2.2.2",
    "react-dom": "^15.2.1",
    "react-textarea-autosize": "^4.0.0",
    "react-height": "^2.0.4",
    "react-motion": "^0.4.2",
    "rimraf": "^2.5.4",
    "stylelint": "^7.0.3"
  },
  "peerDependencies": {
    "classnames": "^2.2.3",
    "lodash.omit": "^4.3.0",
    "monet": "^0.8.10",
    "react": "^15.2.1",
    "react-click-outside": "^2.1.0",
    "react-collapse": "^2.2.2",
    "react-dom": "^15.2.1",
    "react-textarea-autosize": "^4.0.0",
    "react-height": "^2.0.4",
    "react-motion": "^0.4.2"
  },
  "license": "GPL-3.0+"
}
